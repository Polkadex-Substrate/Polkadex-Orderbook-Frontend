{
  "name": "polkadex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "generateIcons": "svgr -d src/components/Icons/ public/img/icons/ --typescript --icon",
    "generateCryptocurrencies": "svgr -d src/components/Cryptocurrencies/ public/img/cryptocurrencies/ --typescript --icon",
    "mockAPI": "tsnd --transpile-only --inspect --respawn --ignore-watch node_modules mocks/mockAPI.js",
    "mockWS": "tsnd --transpile-only --inspect --respawn --ignore-watch node_modules mocks/mockWS.js",
    "start-mock": "npm-run-all -p dev mockAPI mockWS",
    "checkTypes": "tsc --noEmit"
  },
  "dependencies": {
